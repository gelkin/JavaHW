
import java.util.HashMap;
import ru.ifmo.ctddef.mazin.*;

public class Main {
	public static void main(String[] args) {
		
		HashMap<String, Integer> vars = new HashMap<String, Integer>();
		if (args.length > 1) {
			vars.put("x", Integer.parseInt(args[1]));
			vars.put("y", Integer.parseInt(args[2]));
			vars.put("z", Integer.parseInt(args[3]));
		}
		Parser x = new Parser(vars);		
		
		System.out.println(x.Parse(args[0]).evaluate(vars));
	}
}
